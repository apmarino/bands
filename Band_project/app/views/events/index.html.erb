<h1>Events</h1><hr>

<% @events.each do |event| %>

<p>Date: <strong class="value"><%= event.date %></strong></p>
<p>Band: <strong class="value"><%= event.band.name %></strong></p>
<p>Venue: <strong class="value"><%= event.venue.name %></strong></p>
<p>Alcohol Served: <strong class="value"><%= event.alcohol_served %></strong> </p>
<a href="/events/<%= event.id %>">See Event</a><hr>

	
	<%end%>
